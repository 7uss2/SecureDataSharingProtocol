    private SecretKey sk;
    private byte[] b;
    static String key;
    /**
     * Creates new form Update
     */
    public Update() {
        initComponents();
        
        jTextField1.setText(CRA.s);
        
        try {
           kg = KeyGenerator.getInstance("AES");
            kg.init(128);
            sk = kg.generateKey();
            b = sk.getEncoded();
            key = b.toString();
       } catch(Exception e){
           e.printStackTrace();
       }
        
    }

